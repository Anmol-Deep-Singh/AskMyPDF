{
  "name": "test",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "install:all": "npm run install:frontend && npm run install:backend",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm run test",
    "test:backend": "cd backend && npm run test",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anmol-Deep-Singh/test.git"
  },
  "keywords": ["monorepo", "frontend", "backend"],
  "author": "Anmol Deep Singh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Anmol-Deep-Singh/test/issues"
  },
  "homepage": "https://github.com/Anmol-Deep-Singh/test#readme",
  "description": "A monorepo for frontend and backend development.",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}